<?xml version="1.0" encoding="UTF-8"?>

<table>
    <name>Incomes</name>
    <filename>SCB_inkomster.csv</filename>
    <description>A table containing data on mean and median income, divided by gender and some age categories.</description>
    <source>SCB</source>
    <year>2018</year>

    <columns>
        <column>
            <identifier>Municipality</identifier>
            <colname>region</colname>
            <alias>Kommun</alias>
            <levels>
                <levelsType>Municipalities</levelsType>
            </levels>
        </column>

        <column>
            <identifier>Age</identifier>
            <colname>ålder</colname>
            <alias>Ålder</alias>
            <levels>
                <levelsType>Character</levelsType>
                <level>
                    <levelName>16-19 år</levelName>
                    <alias>16-19</alias>
                    <alias>16-19 y.o.</alias>
                </level>
                <level>
                    <levelName>20-24 år</levelName>
                    <alias>20-24</alias>
                    <alias>20-24 y.o.</alias>
                </level>
                <level>
                    <levelName>totalt 16+ år</levelName>
                    <alias>Total 16+</alias>
                </level>
                <level>
                    <levelName>totalt 20+ år</levelName>
                    <alias>Total 20+</alias>
                </level>
            </levels>
        </column>

        <column>
            <identifier>Gender</identifier>
            <colname>kön</colname>
            <alias>Kön</alias>
            <levels>
                <levelsType>Character</levelsType>
                <level>
                    <levelName>män</levelName>
                    <alias>Men</alias>
                    <alias>Män</alias>
                    <alias>Male</alias>
                </level>
                <level>
                    <levelName>kvinnor</levelName>
                    <alias>Women</alias>
                    <alias>Kvinnor</alias>
                    <alias>Female</alias>
                </level>
                <level>
                    <levelName>totalt</levelName>
                    <alias>Totalt</alias>
                    <alias>Total</alias>
                </level>
            </levels>
        </column>

        <column>
            <identifier>MeanOrMedian</identifier>
            <colname>MedTyp</colname>
            <alias>MedelvärdeEllerMedian</alias>
            <levels>
                <levelsType>Character</levelsType>
                <level>
                    <levelName>Medelvärde</levelName>
                    <alias>Mean</alias>
                </level>
                <level>
                    <levelName>Median</levelName>
                </level>
        </column>

        <valueColumn>
            <colname>X2018</colname>
        </valueColumn>
    </columns>
</table>