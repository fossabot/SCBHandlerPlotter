<?xml version="1.0" encoding="UTF-8"?>

<table>
    <name>AgeGender</name>
    <filename>SCB_alderkon.csv</filename>
    <title>Ages and genders</title>
    <description>A table containing data on population, divided by age, gender, and municipality.</description>
    <source>SCB</source>
    <year>2019</year>

    <columns>
        <column>
            <identifier>Municipality</identifier>
            <colname>region</colname>
            <alias>Kommun</alias>
            <levels>
                <levelsType>Municipalities</levelsType>
            </levels>
        </column>

        <column>
            <identifier>Age</identifier>
            <colname>ålder</colname>
            <alias>Ålder</alias>
            <levels>
                <levelsType>NumericRange</levelsType>
                <minLevel>0</minLevel>
                <maxLevel>100</maxLevel>
            </levels>
        </column>

        <column>
            <identifier>Gender</identifier>
            <colname>kön</colname>
            <alias>Kön</alias>
            <levels>
                <levelsType>Character</levelsType>
                <level>
                    <levelName>män</levelName>
                    <alias>Men</alias>
                    <alias>Män</alias>
                    <alias>Male</alias>
                </level>
                <level>
                    <levelName>kvinnor</levelName>
                    <alias>Women</alias>
                    <alias>Kvinnor</alias>
                    <alias>Female</alias>
                </level>
            </levels>
        </column>

        <valueColumn>
            <colname>X2019</colname>
        </valueColumn>
    </columns>
</table>