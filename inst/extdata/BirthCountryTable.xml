<?xml version="1.0" encoding="UTF-8"?>

<table>
    <name>BirthRegion</name>
    <filename>SCB_inrikesvsutrikesfodda.csv</filename>
    <description>A table containing data on country of birth (Sweden or not Sweden), divided by age and gender.</description>
    <source>SCB</source>
    <year>2019</year>

    <columns>
        <column>
            <identifier>Municipality</identifier>
            <colname>region</colname>
            <alias>Kommun</alias>
            <levels>
                <levelsType>Municipalities</levelsType>
            </levels>
        </column>

        <column>
            <identifier>Age</identifier>
            <colname>ålder</colname>
            <alias>Ålder</alias>
            <levels>
                <levelsType>NumericRange</levelsType>
                <minLevel>12</minLevel>
                <maxLevel>25</maxLevel>
            </levels>
        </column>

        <column>
            <identifier>Gender</identifier>
            <colname>kön</colname>
            <alias>Kön</alias>
            <levels>
                <levelsType>Character</levelsType>
                <level>
                    <levelName>män</levelName>
                    <alias>Men</alias>
                    <alias>Män</alias>
                    <alias>Male</alias>
                </level>
                <level>
                    <levelName>kvinnor</levelName>
                    <alias>Women</alias>
                    <alias>Kvinnor</alias>
                    <alias>Female</alias>
                </level>
            </levels>
        </column>

        <column>
            <identifier>BirthRegion</identifier>
            <colname>födelseregion</colname>
            <alias>Födelseregion</alias>
            <levels>
                <levelsType>Character</levelsType>
                <level>
                    <levelName>Född i Sverige</levelName>
                    <alias>Sverige</alias>
                    <alias>Born in Sweden</alias>
                    <alias>Sweden</alias>
                </level>
                <level>
                    <levelName>Utrikes född</levelName>
                    <alias>Inte Sverige</alias>
                    <alias>Born outside Sweden</alias>
                    <alias>Not Sweden</alias>
                </level>
            </levels>
        </column>

        <valueColumn>
            <colname>X2019</colname>
        </valueColumn>
    </columns>
</table>