                    <alias>Buildings</alias>
                </level>
                <level>
                    <levelName>betesmark</levelName>
                    <alias>Betesmark</alias>
                    <alias>Pasture</alias>
                </level>
                <level>
                    <levelName>golfbanor och skidpistar</levelName>
                    <alias>Golfbanor och skidpistar</alias>
                    <alias>Golf courses and ski pistes</alias>
                </level>
                <level>
                    <levelName>skogsmark, improduktiv</levelName>
                    <alias>Improduktiv skogsmark</alias>
                    <alias>Unproductive forest</alias>
                </level>
                <level>
                    <levelName>skogsmark, produktiv</levelName>
                    <alias>Produktiv skogsmark</alias>
                    <alias>Productive forest</alias>
                </level>
                <level>
                    <levelName>total landareal</levelName>
