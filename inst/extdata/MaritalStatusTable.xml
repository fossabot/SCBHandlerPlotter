<?xml version="1.0" encoding="UTF-8"?>

<table>
    <name>MaritalStatus</name>
    <filename>SCB_civilstand.csv</filename>
    <title>Marital status</title>
    <description>A table containing data on marital status, divided by age, gender, and municipality.</description>
    <source>SCB</source>
    <year>2019</year>

    <columns>
        <column>
            <identifier>Municipality</identifier>
            <colname>region</colname>
            <alias>Kommun</alias>
            <levels>
                <levelsType>Municipalities</levelsType>
            </levels>
        </column>

        <column>
            <identifier>Age</identifier>
            <colname>ålder</colname>
            <alias>Ålder</alias>
            <levels>
                <levelsType>NumericRange</levelsType>
                <minLevel>12</minLevel>
                <maxLevel>25</maxLevel>
            </levels>
        </column>

        <column>
            <identifier>Gender</identifier>
            <colname>kön</colname>
            <alias>Kön</alias>
            <levels>
                <levelsType>Character</levelsType>
                <level>
                    <levelName>män</levelName>
                    <alias>Men</alias>
                    <alias>Män</alias>
                    <alias>Male</alias>
                </level>
                <level>
                    <levelName>kvinnor</levelName>
                    <alias>Women</alias>
                    <alias>Kvinnor</alias>
                    <alias>Female</alias>
                </level>
            </levels>
        </column>

        <column>
            <identifier>MaritalStatus</identifier>
            <colname>civilstånd</colname>
            <alias>Civilstånd</alias>
            <levels>
                <levelsType>Character</levelsType>
                <level>
                    <levelName>ogifta</levelName>
                    <alias>Unmarried</alias>
                    <alias>Never married</alias>
                    <alias>Ogifta</alias>
                    <alias>Aldrig gifta</alias>
                </level>
                <level>
                    <levelName>gifta</levelName>
                    <alias>Married</alias>
                    <alias>Gifta</alias>
                </level>
                <level>
                    <levelName>skilda</levelName>
                    <alias>Divorced</alias>
                    <alias>Skilda</alias>
                </level>
                <level>
                    <levelName>änkor/änklingar</levelName>
                    <alias>WidowErs</alias>
                    <alias>Änk(or/lingar)</alias>
                    <alias>Spouse deceased</alias>
                </level>
            </levels>
        </column>

        <valueColumn>
            <colname>X2019</colname>
        </valueColumn>
    </columns>
</table>